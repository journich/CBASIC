10 REM GUESS THE NUMBER GAME
20 PRINT "I'M THINKING OF A NUMBER BETWEEN 1 AND 100"
30 N = INT(RND(1)*100)+1
40 G = 0
50 INPUT "YOUR GUESS"; X
60 G = G + 1
70 IF X = N THEN 110
80 IF X < N THEN PRINT "TOO LOW"
90 IF X > N THEN PRINT "TOO HIGH"
100 GOTO 50
110 PRINT "YOU GOT IT IN"; G; "GUESSES!"
120 END
