10 REM Test leap year
20 DATA 2000, 2001, 2004, 2100
30 FOR I = 1 TO 4
40 READ Y
50 LET L = 0
60 IF Y - INT(Y/4)*4 = 0 THEN L = 1
70 IF Y - INT(Y/100)*100 = 0 THEN L = 0
80 IF Y - INT(Y/400)*400 = 0 THEN L = 1
90 IF L = 1 THEN PRINT Y; " IS LEAP YEAR"
100 IF L = 0 THEN PRINT Y; " IS NOT LEAP YEAR"
110 NEXT I
120 END
