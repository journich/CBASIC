10 REM IF/THEN/ELSE TEST
20 PRINT "IF/THEN/ELSE TEST"
30 A = 5
40 IF A = 5 THEN PRINT "A EQUALS 5"
50 IF A < 10 THEN PRINT "A IS LESS THAN 10"
60 IF A > 3 THEN PRINT "A IS GREATER THAN 3"
70 IF A <> 4 THEN PRINT "A IS NOT 4"
80 IF A >= 5 THEN PRINT "A >= 5"
90 IF A <= 5 THEN PRINT "A <= 5"
100 B = 3
110 IF A > B THEN PRINT "A > B" ELSE PRINT "A <= B"
120 IF A < B THEN PRINT "A < B" ELSE PRINT "A >= B"
130 END
