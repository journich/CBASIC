10 REM TEST ARRAYS
20 PRINT "1D ARRAY:"
30 DIM A(10)
40 FOR I = 0 TO 10
50 A(I) = I * I
60 NEXT I
70 FOR I = 0 TO 10
80 PRINT A(I);
90 NEXT I
100 PRINT
110 PRINT "2D ARRAY:"
120 DIM B(3,3)
130 FOR I = 0 TO 3
140 FOR J = 0 TO 3
150 B(I,J) = I * 4 + J
160 NEXT J
170 NEXT I
180 FOR I = 0 TO 3
190 FOR J = 0 TO 3
200 PRINT B(I,J);
210 NEXT J
220 PRINT
230 NEXT I
240 PRINT "STRING ARRAY:"
250 DIM C$(5)
260 C$(0) = "ZERO"
270 C$(1) = "ONE"
280 C$(2) = "TWO"
290 C$(3) = "THREE"
300 C$(4) = "FOUR"
310 FOR I = 0 TO 4
320 PRINT I; "="; C$(I)
330 NEXT I
