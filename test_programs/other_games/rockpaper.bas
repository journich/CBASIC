10 REM ROCK PAPER SCISSORS
20 PRINT "ROCK, PAPER, SCISSORS"
30 PRINT "ENTER: R, P, OR S"
40 W = 0
50 L = 0
60 INPUT "YOUR CHOICE"; Y$
70 IF Y$ = "Q" THEN PRINT "WINS:"; W; " LOSSES:"; L: END
80 C = INT(RND(1)*3)+1
90 IF C = 1 THEN C$ = "R"
100 IF C = 2 THEN C$ = "P"
110 IF C = 3 THEN C$ = "S"
120 PRINT "I CHOSE: "; C$
130 IF Y$ = C$ THEN PRINT "TIE!": GOTO 60
140 IF Y$ = "R" AND C$ = "S" THEN PRINT "YOU WIN!": W = W + 1: GOTO 60
150 IF Y$ = "P" AND C$ = "R" THEN PRINT "YOU WIN!": W = W + 1: GOTO 60
160 IF Y$ = "S" AND C$ = "P" THEN PRINT "YOU WIN!": W = W + 1: GOTO 60
170 PRINT "I WIN!"
180 L = L + 1
190 GOTO 60
