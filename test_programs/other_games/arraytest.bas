10 REM ARRAY TEST PROGRAM
20 DIM A(10), B(5,5)
30 PRINT "TESTING ARRAYS"
40 FOR I = 1 TO 10
50 A(I) = I * 2
60 NEXT I
70 PRINT "1D ARRAY:"
80 FOR I = 1 TO 10
90 PRINT A(I);
100 NEXT I
110 PRINT
120 PRINT "2D ARRAY:"
130 FOR I = 1 TO 5
140 FOR J = 1 TO 5
150 B(I,J) = I * J
160 NEXT J
170 NEXT I
180 FOR I = 1 TO 5
190 FOR J = 1 TO 5
200 PRINT B(I,J);
210 NEXT J
220 PRINT
230 NEXT I
240 END
