10 REM TEST LOGICAL OPERATORS
20 PRINT "AND:"
30 IF 1 AND 1 THEN PRINT "1 AND 1 = TRUE"
40 IF 1 AND 0 THEN PRINT "1 AND 0 = TRUE"
50 IF 0 AND 0 THEN PRINT "0 AND 0 = TRUE"
60 PRINT "OR:"
70 IF 1 OR 1 THEN PRINT "1 OR 1 = TRUE"
80 IF 1 OR 0 THEN PRINT "1 OR 0 = TRUE"
90 IF 0 OR 0 THEN PRINT "0 OR 0 = TRUE"
100 PRINT "NOT:"
110 IF NOT 0 THEN PRINT "NOT 0 = TRUE"
120 IF NOT 1 THEN PRINT "NOT 1 = TRUE"
130 PRINT "COMPOUND:"
140 A = 5
150 B = 10
160 IF A > 3 AND B < 20 THEN PRINT "5>3 AND 10<20 = TRUE"
170 IF A > 10 OR B > 5 THEN PRINT "5>10 OR 10>5 = TRUE"
180 IF NOT (A > 10) THEN PRINT "NOT (5>10) = TRUE"
190 END
