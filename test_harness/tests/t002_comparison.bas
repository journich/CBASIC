10 REM TEST COMPARISON OPERATORS
20 A = 5
30 B = 10
40 C = 5
50 PRINT "EQUAL:"
60 IF A = C THEN PRINT "5=5 TRUE"
70 IF A = B THEN PRINT "5=10 TRUE"
80 PRINT "NOT EQUAL:"
90 IF A <> B THEN PRINT "5<>10 TRUE"
100 IF A <> C THEN PRINT "5<>5 TRUE"
110 PRINT "LESS THAN:"
120 IF A < B THEN PRINT "5<10 TRUE"
130 IF B < A THEN PRINT "10<5 TRUE"
140 PRINT "GREATER THAN:"
150 IF B > A THEN PRINT "10>5 TRUE"
160 IF A > B THEN PRINT "5>10 TRUE"
170 PRINT "LESS OR EQUAL:"
180 IF A <= C THEN PRINT "5<=5 TRUE"
190 IF A <= B THEN PRINT "5<=10 TRUE"
200 IF B <= A THEN PRINT "10<=5 TRUE"
210 PRINT "GREATER OR EQUAL:"
220 IF A >= C THEN PRINT "5>=5 TRUE"
230 IF B >= A THEN PRINT "10>=5 TRUE"
240 IF A >= B THEN PRINT "5>=10 TRUE"
250 END
