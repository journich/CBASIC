10 REM FOR LOOP EDGE CASES
20 PRINT "FOR LOOP TEST"
30 PRINT "COUNTING UP:"
40 FOR I = 1 TO 5
50 PRINT I;
60 NEXT I
70 PRINT
80 PRINT "COUNTING DOWN:"
90 FOR I = 5 TO 1 STEP -1
100 PRINT I;
110 NEXT I
120 PRINT
130 PRINT "STEP 2:"
140 FOR I = 0 TO 10 STEP 2
150 PRINT I;
160 NEXT I
170 PRINT
180 PRINT "FRACTIONAL STEP:"
190 FOR X = 0 TO 1 STEP 0.2
200 PRINT X;
210 NEXT X
220 PRINT
230 END
