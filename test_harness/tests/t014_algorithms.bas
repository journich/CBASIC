10 REM TEST ALGORITHMS
20 PRINT "FACTORIAL:"
30 FOR N = 0 TO 10
40 F = 1
50 FOR I = 1 TO N
60 F = F * I
70 NEXT I
80 PRINT N; "! ="; F
90 NEXT N
100 PRINT "FIBONACCI:"
110 A = 0
120 B = 1
130 PRINT A;
140 FOR I = 1 TO 15
150 PRINT B;
160 C = A + B
170 A = B
180 B = C
190 NEXT I
200 PRINT
210 PRINT "PRIMES TO 50:"
220 FOR N = 2 TO 50
230 P = 1
240 FOR I = 2 TO SQR(N)
250 IF N/I = INT(N/I) THEN P = 0
260 NEXT I
270 IF P = 1 THEN PRINT N;
280 NEXT N
290 PRINT
300 END
