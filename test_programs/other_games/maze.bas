10 REM SIMPLE MAZE GENERATOR
20 W = 20
30 H = 10
40 DIM M$(H,W)
50 FOR Y = 1 TO H
60 FOR X = 1 TO W
70 IF RND(1) > 0.3 THEN M$(Y,X) = " " ELSE M$(Y,X) = "#"
80 NEXT X
90 NEXT Y
100 M$(1,1) = "S"
110 M$(H,W) = "E"
120 PRINT "MAZE (S=START, E=END)"
130 FOR Y = 1 TO H
140 L$ = ""
150 FOR X = 1 TO W
160 L$ = L$ + M$(Y,X)
170 NEXT X
180 PRINT L$
190 NEXT Y
200 END
